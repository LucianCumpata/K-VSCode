{
    "name": "kframework",
    "displayName": "K",
    "description": "K language support for VSC",
    "version": "0.1.4",
    "publisher": "clv",
    "author": {
        "name": "Lucian Cumpata",
        "email": "cumpata.lu@outlook.com",
        "url": "https://github.com/LucianCumpata"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "LICENSE.txt",
    "galleryBanner": {
        "color": "#2c2e7e",
        "theme": "dark"
    },
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/LucianCumpata/K-VSCode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LucianCumpata/K-VSCode.git"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kframework",
                "aliases": [
                    "Kframework",
                    "K",
                    "k"
                ],
                "extensions": [
                    ".k"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kframework",
                "scopeName": "text.k",
                "path": "./syntaxes/k.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "kframework",
                "path": "./snippets/k.json"
            }
        ]
    },
    "__metadata": null
}